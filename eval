int	main(int argc, char *argv[])
{
	if (argc == 2)
		printf("%ld", ft_strlen(argv[1]));
	printf("\n");
	return (0);
}

==================

#include <stdio.h>

int	main(int argc, char *argv[])
{
	if (argc == 2)
		printf("%d", ft_isalpha((int)argv[1][0]));
	printf("\n");
	return (0);
}

==================

#include <stdio.h>

int	main(int argc, char *argv[])
{
	if (argc == 2)
		printf("%d", ft_isdigit(argv[1][0]));
	printf("\n");
	return (0);
}

